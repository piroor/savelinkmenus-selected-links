<?xml version="1.0"?>

<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">

    <Description about="urn:mozilla:install-manifest">
        <em:id>savelinkmenus-selected-links@piro.sakura.ne.jp</em:id>
        <em:type>2</em:type>
        <em:name>Save Link Menus</em:name>
        <em:version>0.1.1</em:version>
        <em:bootstrap>true</em:bootstrap>
        <em:description>Adds 'Save Link' and 'Save Page' menus</em:description>
        <em:creator>TakaChan</em:creator>
        <em:optionsURL>chrome://savelinkmenus/content/settings.xul</em:optionsURL>

        <em:localized>
            <Description>
                <em:locale>ja</em:locale>
                <em:name>Save Link Menus</em:name>
                <em:description>「リンク先を保存」と「ページを保存」メニューを追加します</em:description>
                <em:creator>TakaChan</em:creator>
            </Description>
        </em:localized>

        <!-- Firefox for Android -->
        <em:targetApplication>
            <Description>
                <em:id>{aa3c5121-dab2-40e2-81ca-7ea25febc110}</em:id>
                <em:minVersion>24.0</em:minVersion>
                <em:maxVersion>28.0a1</em:maxVersion>
            </Description>
        </em:targetApplication>
    </Description>
</RDF>
